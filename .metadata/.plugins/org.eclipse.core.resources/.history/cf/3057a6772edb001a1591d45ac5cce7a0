server.port = 8088

# Integração com o BD
#--------------- ITENS OBRIGATORIOS
# 1 - Usuario do MySQL
spring.datasource.username = root

# 2 - Senha do usuario
spring.datasource.password = And3rs0n@

# 3 - URL do banco
spring.datasource.url = jdbc:mysql://localhost:3306/projeto

# 4 - "Dialeto" (driver) que será usado
spring.jpa.properties.hibernate.dialect = org.hibernate.dialect.MySQL5Dialect


